---
import type { Block } from "~/types/block";
import { TextBlock } from "./TextBlock";

// TODO: remove Exclude once more block types are added
export type Props = Exclude<Block, undefined>;

const { _type, portableText } = Astro.props;

// console.log({ _type, portableText });
---

{_type === "textBlock" && <TextBlock value={portableText} client:visible />}
