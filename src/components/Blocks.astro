---
import type { Block as BlockProps } from '~/types/block';
import Block from './Block.astro';
import type { FormQueryParams } from '~/types/form';

interface Props extends FormQueryParams {
  blocks: BlockProps[];
}

const { blocks, slug, pageType } = Astro.props;
---

{
  blocks.map((block) =>
    block._type === 'form' ? (
      <Block {...block} slug={slug} pageType={pageType} />
    ) : (
      <Block {...block} />
    )
  )
}
